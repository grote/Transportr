<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <color name="holo_red_light">#ffff4444</color>
    <color name="walking">#ffea00</color>

    <!-- material theme colors -->
    <color name="primary">#f44336</color>
    <color name="primary_dark">#b71c1c</color>
    <color name="accent">#e53935</color>

    <!-- To enable accent color in settings for devices with API <21 -->
    <color name="preference_fallback_accent_color" tools:override="true" tools:ignore="UnusedResources">@color/accent</color>

    <color name="drawableTintDark">#ffffffff</color>
    <color name="drawableTintLight">@color/md_grey_600</color>

    <color name="buttonColor">@color/md_blue_500</color>

    <!-- GPS FAB -->
    <color name="fabBackground">@color/cardview_light_background</color>
    <color name="fabBackgroundMoved">@color/md_blue_700</color>
    <color name="fabForegroundInitial">@color/drawableTintLight</color>
    <color name="fabForegroundFollow">@color/fabBackgroundMoved</color>
    <color name="fabForegroundMoved">@color/md_white_1000</color>

    <color name="cardview_dark_background" tools:override="true">#FF424242</color>
    <color name="cardview_light_background" tools:override="true">#FFFFFFFF</color>

    <!-- Material Drawer -->
    <color name="material_drawer_header_selection_text" tools:ignore="UnusedResources">#FFFFFFFF</color>
    <color name="material_drawer_header_selection_subtext" tools:ignore="UnusedResources">#B3FFFFFF</color>
    <color name="material_drawer_dark_header_selection_text" tools:ignore="UnusedResources">#FFFFFFFF</color>
    <color name="material_drawer_dark_header_selection_subtext" tools:ignore="UnusedResources">#B3FFFFFF</color>

</resources>
